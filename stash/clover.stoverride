name: clover
desc: Replace original configuration file.
dns: #!replace
  enable: true
  ipv6: false
  enhanced-mode: fake-ip
  fake-ip-range: 192.18.0.1/16
  nameserver:
    - 119.29.29.29
    - 119.28.28.28
    - 223.5.5.5
    - 223.6.6.6
  fallback:
    - 114.114.114.114
    - 114.114.115.115
    - 101.226.4.6
    - 218.30.118.6
    - 8.8.8.8
    - 94.140.14.15
    - 94.140.15.16
    - 1.1.1.1
  fake-ip-filter:
    - +.stun.*.*
    - +.stun.*.*.*
    - +.stun.*.*.*.*
    - +.stun.*.*.*.*.*
    - "*.n.n.srv.nintendo.net"
    - +.stun.playstation.net
    - xbox.*.*.microsoft.com
    - "*.*.xboxlive.com"
    - "*.msftncsi.com"
    - "*.msftconnecttest.com"
    - WORKGROUP
rules: #!replace
  - RULE-SET,customize-reject,REJECT
  - RULE-SET,customize-direct,DIRECT
  - RULE-SET,customize-openai,🌄 特殊控制 | OpenAI
  - RULE-SET,customize-brad,🌄 特殊控制 | Brad
  - RULE-SET,customize-copilot,🌄 特殊控制 | Copilot
  - RULE-SET,customize-edge,🌄 特殊控制 | Edge
  - RULE-SET,customize-nodejs,🌄 特殊控制 | Node.js
  - RULE-SET,customize-proxy,🌌 科学上网 | CLOVER
  - RULE-SET,default-applications,DIRECT
  - RULE-SET,default-apple,DIRECT
  - RULE-SET,default-icloud,DIRECT
  - RULE-SET,default-private,DIRECT
  - RULE-SET,default-direct,DIRECT
  - RULE-SET,default-greatfire,🌌 科学上网 | CLOVER
  - RULE-SET,default-gfw,🌌 科学上网 | CLOVER
  - RULE-SET,default-proxy,🌌 科学上网 | CLOVER
  - RULE-SET,default-tld-not-cn,🌌 科学上网 | CLOVER
  - RULE-SET,default-reject,REJECT
  - RULE-SET,default-telegramcidr,🌌 科学上网 | CLOVER,no-resolve
