rules: #!replace
  - PROCESS-NAME,BitComet.exe,🛣️ 科学上网 B
  - PROCESS-NAME,aria2c.exe,DIRECT
  - PROCESS-NAME,Motrix.exe,DIRECT
  - RULE-SET,Customize-Reject,REJECT
  - RULE-SET,Customize-Special,🌤️ 特殊控制 B
  - RULE-SET,Customize-Direct,DIRECT
  - RULE-SET,Customize-Proxy,🛣️ 科学上网 B
  - RULE-SET,Remote-Applications,DIRECT
  - RULE-SET,Remote-Apple,DIRECT
  - RULE-SET,Remote-iCloud,DIRECT
  - RULE-SET,Remote-Reject,REJECT
  - RULE-SET,Remote-Proxy,🛣️ 科学上网 B
  - RULE-SET,Remote-GFW,🛣️ 科学上网 B
  - RULE-SET,Remote-Direct,DIRECT
  - RULE-SET,Remote-Private,DIRECT
  - RULE-SET,Remote-Greatfire,🛣️ 科学上网 B
  - RULE-SET,Remote-Tld-not-cn,🛣️ 科学上网 B
  - RULE-SET,Remote-Telegramcidr,🛣️ 科学上网 B,no-resolve
  - RULE-SET,Remote-Cncidr,DIRECT,no-resolve
  - RULE-SET,Remote-Lancidr,DIRECT,no-resolve
  - GEOIP,LAN,DIRECT,no-resolve
  - GEOIP,CN,DIRECT,no-resolve
  - MATCH,🌊 规则逃逸 B
rule-providers: #!replace
  Remote-Reject:
    type: http
    behavior: domain
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/reject.txt
    path: H:\Proxy Rules\clash\rules\reject.yaml
    interval: 86400
  Remote-Proxy:
    type: http
    behavior: domain
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/proxy.txt
    path: H:\Proxy Rules\clash\rules\proxy.yaml
    interval: 86400
  Remote-Direct:
    type: http
    behavior: domain
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/direct.txt
    path: H:\Proxy Rules\clash\rules\direct.yaml
    interval: 86400
  Remote-Private:
    type: http
    behavior: domain
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/private.txt
    path: H:\Proxy Rules\clash\rules\private.yaml
    interval: 86400
  Remote-GFW:
    type: http
    behavior: domain
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/gfw.txt
    path: H:\Proxy Rules\clash\rules\gfw.yaml
    interval: 86400
  Remote-Greatfire:
    type: http
    behavior: domain
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/greatfire.txt
    path: H:\Proxy Rules\clash\rules\greatfire.yaml
    interval: 86400
  Remote-Tld-not-cn:
    type: http
    behavior: domain
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/tld-not-cn.txt
    path: H:\Proxy Rules\clash\rules\tld-not-cn.yaml
    interval: 86400
  Remote-Telegramcidr:
    type: http
    behavior: ipcidr
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/telegramcidr.txt
    path: H:\Proxy Rules\clash\rules\telegramcidr.yaml
    interval: 86400
  Remote-Cncidr:
    type: http
    behavior: ipcidr
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/cncidr.txt
    path: H:\Proxy Rules\clash\rules\cncidr.yaml
    interval: 86400
  Remote-Lancidr:
    type: http
    behavior: ipcidr
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/lancidr.txt
    path: H:\Proxy Rules\clash\rules\lancidr.yaml
    interval: 86400
  Remote-Applications:
    type: http
    behavior: classical
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/applications.txt
    path: H:\Proxy Rules\clash\rules\applications.yaml
    interval: 86400
  Remote-iCloud:
    type: http
    behavior: domain
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/icloud.txt
    path: H:\Proxy Rules\clash\rules\icloud.yaml
    interval: 86400
  Remote-Apple:
    type: http
    behavior: domain
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/apple.txt
    path: H:\Proxy Rules\clash\rules\apple.yaml
    interval: 86400
  Customize-Special:
    type: http
    behavior: domain
    url: https://raw.githubusercontent.com/dylan127c/proxy-rules/main/customize%20rules/special.yaml
    path: ""
    interval: 86400
  Customize-Direct:
    type: http
    behavior: domain
    url: https://raw.githubusercontent.com/dylan127c/proxy-rules/main/customize%20rules/direct.yaml
    path: ""
    interval: 86400
  Customize-Reject:
    type: http
    behavior: domain
    url: https://raw.githubusercontent.com/dylan127c/proxy-rules/main/customize%20rules/reject.yaml
    path: ""
    interval: 86400
  Customize-Proxy:
    type: http
    behavior: domain
    url: https://raw.githubusercontent.com/dylan127c/proxy-rules/main/customize%20rules/proxy.yaml
    path: ""
    interval: 86400
