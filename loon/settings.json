{
    "rules": "./rules",
    "replacement": {
        "PROCESS-NAME": {
            "^payload:$\n": "",
            "^\\s*#": "#",
            "^\\s{2}-\\s{1}": ""
        },
        "DOMAIN-SUFFIX": {
            "^payload:$\n": "",
            "^\\s*#": "#",
            "^.+?'\\+?\\.?": "DOMAIN-SUFFIX,",
            "'$": ""
        },
        "IP-CIDR": {
            "^payload:$\n": "",
            "^\\s*#": "#",
            "^\\s{2}-\\s{1}'": "IP-CIDR,",
            "'$": ",no-resolve"
        }
    },
    "classical": "PROCESS-NAME",
    "domain": "DOMAIN-SUFFIX",
    "ipcidr": "IP-CIDR",
    "defaultBehavior": "domain",
    "behavior": {
        "classical": [
            "pikpak",
            "applications"
        ],
        "ipcidr": [
            "telegramcidr",
            "lancidr",
            "cncidr"
        ]
    },
    "originalPrefix": "original",
    "originalNative": "../commons/rules/original",
    "additionPrefix": "addition",
    "additionNative": "../commons/rules/addition"
}